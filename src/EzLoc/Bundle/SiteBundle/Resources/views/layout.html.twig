{% extends '::base.html.twig' %}

{% block body %}
    {% block navigation %}
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="container">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header page-scroll">
                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a class="navbar-brand page-scroll" href="#page-top">Location Ile aux Moines</a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                    <ul class="nav navbar-nav navbar-right">
                        <li class="hidden">
                            <a href="#page-top"></a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#portfolio">La maison</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#about">Vivre sur l'ile</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#services">Tarifs</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#map">Carte</a>
                        </li>
                        <li>
                            <a class="page-scroll" href="#contact">Nous Contacter</a>
                        </li>
                    </ul>
                </div>
                <!-- /.navbar-collapse -->
            </div>
            <!-- /.container-fluid -->
        </nav>
    {% endblock %}

    {% block content %}{% endblock %}

    {% block javascripts %}
        <!-- jQuery Version 1.11.0 -->
        <script src="/bundles/ezlocsite/js/jquery-1.11.0.js"></script>

        <!-- Bootstrap Core JavaScript -->
        <script src="/bundles/ezlocsite/js/bootstrap.min.js"></script>

        <!-- Google Map Api -->
        <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
        <script type="text/javascript" src="/bundles/ezlocsite/js/functions.js"></script>

        <!-- Plugin JavaScript -->
        <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
        <script src="/bundles/ezlocsite/js/classie.js"></script>
        <script src="/bundles/ezlocsite/js/cbpAnimatedHeader.js"></script>

        <!-- Contact Form JavaScript -->
        <script src="/bundles/ezlocsite/js/jqBootstrapValidation.js"></script>
        <script src="/bundles/ezlocsite/js/contact_me.js"></script>

        <!-- Custom Theme JavaScript -->
        <script src="/bundles/ezlocsite/js/agency.js"></script>

        <!-- Datetiumer JavaScript -->
        <script type="text/javascript" src="/bundles/ezlocsite/js/moment.js"></script>
        <script type="text/javascript" src="/bundles/ezlocsite/js/fr.js"></script>
        <script type="text/javascript" src="/bundles/ezlocsite/js/bootstrap-datetimepicker.min.js"></script>
        <script type="text/javascript">
            $(function () {
                $('#date_beg').datetimepicker
                ({
                    pickTime: false
                });
            });
            $(function () {
                $('#date_end').datetimepicker
                ({
                    pickTime: false
                });
            });
        </script>

        <!-- Custom Fonts -->
        <link href="/bundles/ezlocsite/font-awesome-4.1.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
        <link href="http://fonts.googleapis.com/css?family=Montserrat:400,700" rel="stylesheet" type="text/css">
        <link href='http://fonts.googleapis.com/css?family=Kaushan+Script' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css'>
        <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,100,300,700' rel='stylesheet' type='text/css'>
    {% endblock %}



{% endblock %}
